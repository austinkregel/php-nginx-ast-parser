{
    "name": "kregel/nginx-parser",
    "description": "A PHP package that uses Bison to parse nginx.conf files for programmatic management",
    "type": "library",
    "require": {
        "mrsuh/php-bison-skeleton": "^1.2",
        "doctrine/lexer": "^3.0",
        "illuminate/support": "^11.13",
        "symfony/var-dumper": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Kregel\\NginxParser\\": "src/"
        },
        "files": [
            "src/Parser.php",
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kregel\\NginxParser\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Austin Kregel",
            "email": "5355937+austinkregel@users.noreply.github.com"
        }
    ]
}
